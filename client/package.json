{
  "name": "scalajs-material-ui",
  "version": "2.3.0",
  "description": "The ScalaJs with React Material UI",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build-dev": "webpack --config webpack.config.js",
    "build-prod": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --host 0.0.0.0",
    "clean": "rimraf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ../ui/users-app/**/**.js"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@material-ui/core": "1.0.0-rc.1",
    "classnames": "2.2.5",
    "cross-env": "5.1.2",
    "i18next": "11.3.2",
    "i18next-browser-languagedetector": "2.2.0",
    "material-ui": "1.0.0-beta.44",
    "material-ui-chip-input": "1.0.0-beta.4",
    "material-ui-icons": "1.0.0-beta.17",
    "material-ui-pickers": "1.0.0-rc.9",
    "moment": "2.22.1",
    "npm": "6.0.0",
    "react": "16.5.1",
    "react-clamp-lines": "1.1.0",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.5.1",
    "react-i18next": "7.6.1",
    "react-popper": "0.10.4",
    "react-router-dom": "4.2.2",
    "react-scroll": "1.7.9",
    "react-select": "1.2.1",
    "validator": "9.4.1",
    "@types/recompose": "0.26.1",
    "recompose": "0.27.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "7.0.0-beta.46",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "closure-webpack-plugin": "^1.1.1",
    "concatenate-files": "^0.1.1",
    "copy-webpack-plugin": "4.5.4",
    "cross-env": "^5.2.0",
    "css-loader": "0.28.9",
    "enzyme": "3.3.0",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.15.0-rc.2",
    "i": "^0.3.6",
    "jest": "22.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.9.4",
    "on-build-webpack": "^0.1.0",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "^4.3.4",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.0",
    "url-loader": "0.6.2",
    "webpack": "4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4"
  }
}
